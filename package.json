{
  "name": "node-backend-skeleton",
  "version": "0.0.1",
  "description": "system use nodejs-express framework as a backend and use mongodb-mongoose to store data",
  "main": "server/server.js",
  "scripts": {
    "start-server": "node server/server.js",
    "test-server": "./node_modules/mocha/bin/_mocha",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-server",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "assert": "latest",
    "body-parser": "^1.17.2",
    "crypto": "latest",
    "dateformat": "latest",
    "express": "^4.15.3",
    "extend": "latest",
    "forever": "^0.15.1",
    "https": "^1.0.0",
    "jwt-simple": "~0.1.0",
    "later": "latest",
    "moment": "latest",
    "mongoose": "^4.10.1",
    "node-sass-chokidar": "^0.0.3",
    "nodemon": "^1.8.1",
    "npm-run-all": "^4.0.2",
    "passport": "latest",
    "passport-local": "latest",
    "passport-local-mongoose": "^4.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "underscore": "latest",
    "view-helpers": "latest"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-connect": "^5.0.0",
    "gulp-develop-server": "^0.5.2",
    "gulp-jshint": "^1.12.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-sass": "^3.1.0",
    "gulp-task-listing": "^1.0.1",
    "gulp-util": "^3.0.8",
    "mocha": "^3.4.2",
    "react-scripts": "1.0.10",
    "yargs": "^8.0.2"
  }
}
